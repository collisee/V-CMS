<%@ Master Language="C#" AutoEventWireup="true" Codebehind="Default.master.cs" Inherits="VietNamNet.Websites.HSBC.Default" %>
<%@ Register Src="UserControls/Header.ascx" TagName="Header" TagPrefix="uc" %>
<%@ Register Src="UserControls/Logo.ascx" TagName="Logo" TagPrefix="uc" %>
<%@ Register Src="UserControls/Banner.ascx" TagName="Banner" TagPrefix="uc" %>
<%@ Register Src="UserControls/Menu.ascx" TagName="Menu" TagPrefix="uc" %>
<%@ Register Src="UserControls/PanelLeft.ascx" TagName="PanelLeft" TagPrefix="uc" %>
<%@ Register Src="UserControls/Footer.ascx" TagName="Footer" TagPrefix="uc" %>
<%@ Register Src="UserControls/GA.ascx" TagName="GA" TagPrefix="uc" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <title>HSBC - Khám phá tiềm năng</title>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <link type="text/css" rel="stylesheet" href="/css/style.css" />

  <script type="text/javascript" src="/js/jquery-1.3.2.js"></script>

  <script type="text/javascript" src="/js/jquery.innerfade.js"></script>

  <script type="text/javascript">
        $(document).ready(function() { 
	        $('#image_rotate').innerfade({ 
		        speed: 'slow', 
		        timeout: 4000, 
		        type: 'sequence', 
		        containerheight: '212px'
	        });
        });
  </script>

</head>
<body>
  <form id="frmDefault" runat="server">
    <uc:Header ID="Header1" runat="server" />
    <div class="main">
      <uc:Logo ID="Logo1" runat="server" />
      <uc:Banner ID="Banner1" runat="server" />
      <uc:Menu ID="Menu1" runat="server" />
      <!-- website content -->
      <div class="website_content">
        <uc:PanelLeft ID="PanelLeft1" runat="server" CategoryAlias="thong-tin-ben-le-cuoc-thi" />
        <asp:ContentPlaceHolder ID="cplhContainer" runat="server">
        </asp:ContentPlaceHolder>
        <div class="clearall">
          &nbsp;</div>
      </div>
      <!-- website content -->
      <uc:Footer ID="Footer1" runat="server" />
    </div>
    <uc:GA ID="GA1" runat="server" />
  </form>
</body>
</html>
